{
  "name": "oc-projet-6",
  "version": "1.0.0",
  "description": "Projet 6 - Cr√©ez un site accessible pour une plateforme de photographes",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch ./scss/main.scss:./css/style.css --style expanded",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass ./scss/main.scss:./css/style.comp.css",
    "concat:css": "concat -o ./css/style.concat.css ./css/normalize.css ./css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.concat.css -o ./css/style.prefix.css",
    "compress:css": "sass ./css/style.prefix.css:./css/style.css --style compressed",
    "delete-temp:css": "rm ./css/style.*.css && rm ./css/style.*.css.map",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css delete-temp:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/werner94fribourg/oc-projet-6.git"
  },
  "keywords": [
    "projet",
    "6",
    "openclassrooms",
    "fisheye"
  ],
  "author": "Werner Schmid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/werner94fribourg/oc-projet-6/issues"
  },
  "homepage": "https://github.com/werner94fribourg/oc-projet-6#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concat": "^1.0.3",
    "live-server": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  }
}
